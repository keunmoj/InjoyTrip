<mapper namespace="com.mycom.myboard.dao.CodeDao">
	<select id="codeList" parameterType="string" resultType="com.mycom.myboard.dto.CodeDto">
		select code, code_name
		  from code
		 where group_code = #{groupCode}
		   and use_yn = 'Y'
	</select>
</mapper>
