<mapper namespace="com.ssafy.trip.user.dao.LoginDao">
	<select id="login" parameterType="string" resultType="com.ssafy.trip.user.dto.UserDto">
		select user_seq, user_name, user_nickname, user_password, user_email, user_profile_image_url, user_register_date
		  from users
		 where user_email = #{userEmail}
	</select>
</mapper>
